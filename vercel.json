{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/blog/index.html"
    },
    {
      "source": "/admin",
      "destination": "/blog-admin/index.html"
    },
    {
      "source": "/apps",
      "destination": "/blog/pages/apps.html"
    },
    {
      "source": "/about",
      "destination": "/blog/pages/about.html"
    },
    {
      "source": "/gallery",
      "destination": "/blog/pages/gallery.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
